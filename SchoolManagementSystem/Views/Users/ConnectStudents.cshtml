@using SchoolManagementSystem.ViewModels;
@model UserVM

@foreach(var student in Model.AssignedStudentsList) {
    @student.LastName;
}

<div class="form-group">
    <label asp-for="AssignedStudentId" class="control-label"></label>
    <select multiple asp-for="AssignedStudentId" class="form-control" asp-items="ViewBag.Students"></select>
</div>
<button type="submit" class="btn btn-primary">Save</button>